    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        },
        {
           "type": "package",
           "package": {
               "name": "drupal/gsb_box",
               "version": "dev-custom",
               "type": "drupal-custom-module",
               "source": {
                   "type": "git",
                   "url": "git@github.com:gsb-public/gsb_box.git",
                   "reference": "1.07"
               }
            }
        },
        {
           "type": "package",
           "package": {
               "name": "drupal/alumni_cas_auth",
               "version": "dev-custom",
               "type": "drupal-custom-module",
               "source": {
                   "type": "git",
                   "url": "git@github.com:gsb-public/alumni_cas_auth.git",
                   "reference": "release-6.09"
               }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "metafizzy/isotope",
                "version": "master",
                "type": "drupal-library", 
                "dist": {
                    "url": "https://raw.githubusercontent.com/metafizzy/isotope/master/dist/isotope.pkgd.js",
                    "type": "file"
                }
            }
        }   
